<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
  <ShortName>Furbooru</ShortName>
  <Description>Furbooru image search</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16" type="image/x-icon">https://furbooru.org/favicon.ico</Image>
  <Image width="64" height="64" type="image/svg+xml">https://furbooru.org/favicon.svg</Image>
  <Url type="text/html" method="get" template="https://furbooru.org/search">
    <Param name="q" value="{searchTerms}"/>
  </Url>
</OpenSearchDescription>
